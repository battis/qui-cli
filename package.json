  },
  "type": "module",
  "scripts": {
    "build": "run-s build:*",
    "build:paths": "monorepo-package-paths -w --author --homepage --repository",
    "build:compile": "lerna run build",
    "release": "run-s release:*",
    "release:build": "run-s build",
